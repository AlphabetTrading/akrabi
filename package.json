{
  "name": "akrabi",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "check-types": "tsc --pretty --noEmit",
    "check-format": "prettier --check .",
    "check-lint": "eslint . --ext ts --ext tsx --ext js",
    "format": "prettier --write .",
    "test-all": "npm run check-format && npm run check-lint && npm run check-types && npm run build",
    "prepare": "husky install"
  },
  "dependencies": {
    "clsx": "^2.0.0",
    "eslint-config-prettier": "^9.0.0",
    "formik": "^2.4.5",
    "framer-motion": "^10.16.4",
    "gsap": "^3.12.2",
    "handlebars": "^4.7.8",
    "husky": "^8.0.3",
    "locomotive-scroll": "^5.0.0-beta.8",
    "next": "^13.5.6",
    "nodemailer": "^6.9.7",
    "prettier": "^3.0.3",
    "react": "^18",
    "react-countup": "^6.5.0",
    "react-dom": "^18",
    "react-icons": "^4.11.0",
    "react-slick": "^0.29.0",
    "slick-carousel": "^1.8.1",
    "swiper": "^10.3.1",
    "yup": "^1.3.2"
  },
  "devDependencies": {
    "@types/locomotive-scroll": "^4.1.1",
    "@types/node": "^20",
    "@types/nodemailer": "^6.4.14",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/react-slick": "^0.23.12",
    "autoprefixer": "^10.4.16",
    "eslint": "^8",
    "eslint-config-next": "13.5.4",
    "husky": "^8.0.0",
    "lint-staged": "^14.0.1",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.3",
    "typescript": "^5"
  },
  "lint-staged": {
    "*.{js, jsx, ts, tsx}": "eslint --cache --fix",
    "*.{js, jsx, ts, tsx, css, md}": "prettier --write"
  }
}
